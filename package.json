  },
  "homepage": "https://github.com/clive/notion-md-publish#readme",
  "dependencies": {
    "@notionhq/client": "^2.2.14",
    "commander": "^12.1.0",
    "js-yaml": "^4.1.0",
    "marked": "^12.0.0",
    "p-limit": "^5.0.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.0",
